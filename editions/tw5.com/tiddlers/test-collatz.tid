\procedure collatz(n) 
<% if [<n>match[1]] %>
  1
<% elseif [<n>remainder[2]match[1]] %>
  <<n>> - ^^@@<<parent-ancestorcount>>@@^^ → <$transclude $variable="collatz" n = {{{ [<n>multiply[3]add[1]] }}}/>
<% else %>
  <<n>> - ^^@@<<parent-ancestorcount>>@@^^ → <$transclude $variable="collatz" n = {{{ [<n>divide[2]] }}}/>
<% endif %>
\end

