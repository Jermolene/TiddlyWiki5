title: $:/core/macros/dumpvariables
tags: $:/tags/Macro

\whitespace trim
\procedure dumpvariables()
<ul>
	<$list filter="[variables[]]" variable="varname">
		<li>
			<strong><code><$text text=<<varname>>/></code></strong>:<br/>
			<$codeblock code={{{ [<varname>getvariable[]] }}}/>
		</li>
	</$list>
</ul>
\end
