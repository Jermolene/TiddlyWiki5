title: $:/config/ui/tag-picker/accept-actions-1

<$list filter="[[$(currentTiddler)$]!contains:tags<selectedItem>]" variable="ignore">
<$action-sendmessage $message="tm-add-tag" $param=<<selectedItem>>/>
</$list>
<$list filter="[[$(currentTiddler)$]contains:tags<selectedItem>]" variable="ignore">
<$action-sendmessage $message="tm-remove-tag" $param=<<selectedItem>>/>
</$list>
<$action-deletetiddler $tiddler=<<__tiddler__>>/>
