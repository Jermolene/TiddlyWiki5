title: $:/core/ui/PageTemplate/pagecontrols

\whitespace trim
\define list-display-filter(tag, prefix)
[all[shadows+tiddlers]tag[$tag$]!has[draft.of]] +[addprefix[$prefix$]!field:text[hide]removeprefix[$prefix$]]
\end
<div class="tc-page-controls">
<$list filter=<<list-display-filter "$:/tags/PageControls" "$:/config/PageControlButtons/Visibility/">> variable="listItem" storyview="pop">
<$set name="tv-config-toolbar-class" filter="[<tv-config-toolbar-class>] [<listItem>encodeuricomponent[]addprefix[tc-btn-]]">
<$transclude tiddler=<<listItem>> mode="inline"/>
</$set>
</$list>
</div>
